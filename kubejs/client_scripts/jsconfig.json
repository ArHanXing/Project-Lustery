{
    "include": [
        "./**/*.js",
        "./**/*.ts",
        null
    ],
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "allowJs": true,
        "checkJs": false,
        "target": "ES2015",
        "disableSizeLimit": true,
        "noEmit": true,
        "assumeChangesOnlyAffectDirectDependencies": true,
        "rootDir": ".",
        "baseUrl": "../../.probe/client/probe-types",
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "lib": [
            "ES2015",
            "ES2016.Array.Include",
            "ES2017.Object",
            "ES5"
        ],
        "typeRoots": [
            "../../.probe/packages",
            "../../.probe/client",
            "../../.probe/client/probe-types"
        ]
    }
}